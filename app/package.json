{
  "name": "icestudio",
  "version": "0.3.1-dev",
  "description": "Experimental graphic editor for open FPGAs",
  "author": "Jes√∫s Arroyo Torrens <jesus.jkhlg@gmail.com>",
  "repository": "https://github.com/FPGAwars/icestudio",
  "license": "GPL-2.0",
  "main": "index.html",
  "single-instance": false,
  "window": {
    "width": 900,
    "height": 620,
    "min_width": 800,
    "min_height": 200,
    "toolbar": true,
    "resizable": true,
    "position": "center",
    "icon": "resources/images/icestudio-logo.png"
  },
  "chromium-args": "--enable-logging",
  "apio": {
    "min": "0.3.0rc1",
    "max": "0.4.0",
    "extras": [
      "blackiceprog",
      "tinyfpgab"
    ],
    "external": "",
    "develop": false
  },
  "collection": "0.3.0",
  "engines": {
    "node": ">= 0.10.0"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "angular-gettext-tools": "2.3.5",
    "copy-paste": "^1.3.0",
    "extract-zip": "^1.6.5",
    "fs-extra": "^1.0.0",
    "getos": "^3.0.1",
    "glob": "^7.1.2",
    "is-online": "^5.2.0",
    "jquery": "^3.2.1",
    "marked": "^0.3.12",
    "node-emoji": "^1.8.1",
    "node-lang-info": "^0.2.1",
    "rmdir": "^1.2.0",
    "rsyncwrapper": "^2.0.1",
    "sha1": "^1.1.1",
    "ssh-exec": "^2.0.0",
    "sudo-prompt": "^7.1.0",
    "svgo": "~0.7.2",
    "temporary": "0.0.8"
  },
  "readme": "../README.md",
  "keywords": [
    "FPGA",
    "open",
    "editor",
    "hardware"
  ]
}
