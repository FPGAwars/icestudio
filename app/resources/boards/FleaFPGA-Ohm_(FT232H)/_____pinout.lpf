IOBUF PORT "PS2_clk1" IO_TYPE=LVCMOS25 PULLMODE=UP ;
IOBUF PORT "PS2_data1" IO_TYPE=LVCMOS25 PULLMODE=UP ;
LOCATE COMP "PS2_clk1" SITE "K2" ;
