IOBUF PORT "PS2_clk2" PULLMODE=UP IO_TYPE=LVCMOS25 ;
IOBUF PORT "PS2_data2" PULLMODE=UP IO_TYPE=LVCMOS25 ;
LOCATE COMP "PS2_clk2" SITE "N2" ;
LOCATE COMP "PS2_data2" SITE "M1" ;
IOBUF PORT "PS2_enable" IO_TYPE=LVCMOS25 PULLMODE=NONE ;
LOCATE COMP "PS2_enable" SITE "N1" ;
